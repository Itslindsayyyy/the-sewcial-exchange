<div id="<%= dom_id swap_task %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Swap:</strong>
    <%= swap_task.swap_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Task name:</strong>
    <%= swap_task.task_name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Due date:</strong>
    <%= swap_task.due_date %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Assigned to user:</strong>
    <%= swap_task.assigned_to_user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Completed:</strong>
    <%= swap_task.completed? ? "Yes" : "No" %>
  </div>
</div>
