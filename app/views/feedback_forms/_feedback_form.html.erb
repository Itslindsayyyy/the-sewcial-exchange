<div id="<%= dom_id feedback_form %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= feedback_form.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Category:</strong>
    <%= feedback_form.category %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Message:</strong>
    <%= feedback_form.message %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Submitted at:</strong>
    <%= feedback_form.submitted_at %>
  </div>
</div>
